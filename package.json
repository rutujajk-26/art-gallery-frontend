    "@react-three/fiber": "^8.15.19",
    "@studio-freight/lenis": "^1.0.42",
    "colorthief": "^2.4.0",
    "framer-motion": "^11.18.2",
    "gsap": "^3.12.5",
    "lenis": "^1.2.3",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-intersection-observer": "^9.8.1",
    "react-masonry-css": "^1.0.16",
