    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "postcss": "^8.4.35",
    "rollup": "^3.29.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
